import random
import numpy
import time

maxnum = int(input('Max number: '))
numofnum = int(input('How many numbers? '))

numlist = numpy.random.randint(maxnum, size=numofnum)
print('List without sorting')
print(*numlist, sep = ', ')
time.sleep(2)

numlist.sort()
print('With sorting (ascending order)')
time.sleep(2)
print(*numlist, sep = ', ')
